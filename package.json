{
  "name": "erpnext_autoinstall",
  "description": "Frappe application for automatic setup of ERPNext.",
  "author": "Monogramm",
  "license": "AGPL",
  "dependencies": {
    "cypress": "^4.12.0",
    "gitmoji-changelog": "^2.1.0",
    "lodash": "^4.17.21",
    "mem": "^6.1.0",
    "minimist": "^1.2.6",
    "npm-registry-fetch": "^8.1.3",
    "remark-cli": "^7.0.1",
    "remark-lint": "^6.0.5",
    "remark-lint-list-item-indent": "^1.0.4",
    "remark-preset-lint-recommended": "^3.0.3",
    "snyk": "^1.323.1"
  },
  "scripts": {
    "lint-md": "remark .",
    "gitmoji-changelog": "gitmoji-changelog --preset generic && remark CHANGELOG.md -o",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "lint-list-item-indent"
    ]
  },
  "snyk": true
}
